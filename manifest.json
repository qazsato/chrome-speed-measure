{
	"name": "SpeedMeasure",
	"version": "1.0",
	"manifest_version": 2,

	"description": "Web速度計測ツール",
	"content_scripts": [{
		"matches": ["http://*/*", "https://*/*"],
		"js": ["content_script.js"],
		"run_at" : "document_end"
	}],

	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "速度はかるくん",
		"default_popup": "index.html"
	}
}